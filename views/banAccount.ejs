        <h1 class="text-uppercase" style="text-align: center;color: #a7871a;">ban account</h1>
    </section>
    <section>
        <div class="col-md-12 search-table-col" style="margin-top: -168px;">
            <div><input type="text" class="search form-control" placeholder="Search by typing here.."></div><span class="counter pull-right"></span>
            <div class="table-responsive table-bordered table table-hover table-bordered results">
                <table class="table table-bordered table-hover">
                    <thead class="bill-header cs">
                        <tr>
                            <th id="trs-hd" class="col-lg-1" style="background: #a7871a;width: 654px;">Reported user</th>
                            <th class="text-uppercase col-lg-2" id="trs-hd" style="background: #a7871a;width: 766px;">Reported By</th>
                            <th class="text-uppercase col-lg-2" id="trs-hd" style="background: #a7871a;width: 684px;">Description</th>
                            <th id="trs-hd" class="col-lg-2" style="background: #a7871a;width: 275px;">Action</th>
                        </tr>
                    </thead>
                    
                    <tbody>
                        <% for(var i = 0; i < reportsArray.length; i++){ %>
                        <tr>
                            <% for(var j = 0; j < usersArray.length; j++){ %>
                                <% if (reportsArray[i].uid == usersArray[j].uid) {%>
                                    <td class="text-center" id="userName" name="userName"><%= usersArray[j].first_name + " " + usersArray[j].last_name%></td>
                                <% } %>
                            <% } %>

                            <% for(var k = 0; k < charitiesArray.length; k++){ %>
                                <% if (reportsArray[i].cid == charitiesArray[k].id) {%>
                                    <td class="text-center" id="userLocation" name="userLocation"><%= charitiesArray[k].name %></td>
                                <% } %>
                            <% } %>

                            
                            <td class="text-center" id="userStatus" name="userStatus"><%= reportsArray[i].description %></td>
                            
                            <% for(var j = 0; j < usersArray.length; j++){ %>
                                <% if (reportsArray[i].uid == usersArray[j].uid) {%>
                                    <% if (usersArray[j].status == "Active") {%>
                                        <td class="text-center"><a class="btn btn-danger text-center" style="margin-left: 5px;" href='/banAccount/ban/<%=usersArray[j].id%>'">Ban</a></td>    
                                    <% } else {%>
                                        <td class="text-center"><a class="btn btn-danger text-center" style="margin-left: 5px;background: #1c664a;border-color: #1c664a;" href='/banAccount/unban/<%=usersArray[j].id%>'">Unban</a></td>
                                    <%}%>
                                    
                                <% } %>
                            <% } %>


                        </tr>
                        <%}%>
                    </tbody>
                </table>
            </div>
        </div>
    </section>
